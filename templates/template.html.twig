<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="{{ asset('build/app.css') }}">
</head>
<body>

<div class="page">
    <div class="page-main">
        <div class="header">
            <div class="container">
                <div class="navbar p-0">
                    <a class="navbar-brand" href="{{ path('index.get') }}">
                        <img src="{{ asset('build/assets/images/logo.svg') }}"
                             class="navbar-brand-img" alt="dashtainer.com"></a>
                    <div class="nav order-lg-2">
                        <div class="dropdown ml-3">
                            <a href="#" class="nav-link" data-toggle="dropdown">
                                <img src="{{ asset('build/assets/images/avatar.png') }}"
                                     class="avatar" />
                                <span class="ml-2 d-none d-lg-block"></span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right">
                                <a class="dropdown-item" href="#">
                                    <span>Settings</span>
                                </a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item"
                                   href="{{ path('fos_user_security_logout') }}">Sign out</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="header-nav d-none d-lg-flex">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col">
                        <ul class="nav nav-tabs">
                            <li class="nav-item">
                                <a href="{{ path('index.get') }}"
                                   class="nav-link active"><i class="fe fe-home"></i> Home</a>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link"><i class="fe fe-box"></i> Projects</a>
                                <div class="nav-submenu nav">
                                    <a href="#" class="nav-item ">Create a Project</a>
                                    <div class="dropdown-divider"></div>
                                    <a href="#" class="nav-item ">Charts</a>
                                </div>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link"><i class="fe fe-calendar"></i> Services</a>
                                <div class="nav-submenu nav">
                                    <a href="#" class="nav-item">Create a Service</a>
                                    <div class="dropdown-divider"></div>
                                    <a href="#" class="nav-item ">Store</a>
                                </div>
                            </li>
                            <li class="nav-item">
                                <a href="javascript:void(0)" class="nav-link"><i class="fe fe-file"></i> Pages</a>
                                <div class="nav-submenu nav">
                                    <a href="#" class="nav-item ">Profile</a>
                                </div>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link"><i class="fe fe-check-square"></i> Forms</a>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link"><i class="fe fe-image"></i> Gallery</a>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link"><i class="fe fe-file-text"></i> Documentation</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <div class="page-content">
            <div class="container">
                {% block content %}{% endblock %}
            </div>
        </div>
    </div>

    <footer class="footer">
        <div class="container">
            <div class="row">
                <div class="col-auto mr-auto">
                    Made with <i class="fe fe-heart"></i>
                    by <a href="https://jtreminio.com">Juan Treminio</a>
                </div>
                <div class="col-auto">
                    Theme by <a href="https://codecalm.net" target="_blank">codecalm.net</a>
                </div>
            </div>
        </div>
    </footer>
</div>


<div id="ajax-spinner" class="hidden invisible modal-backdrop in">
    <i class="fa fa-spinner fa-spin" aria-hidden="true"></i>
</div>

<div class="modal" id="dynamic-modal" tabindex="-1" role="dialog">
    <div class="modal-dialog"><div class="modal-content"></div></div>
</div>

<script src="{{ asset('build/app.js') }}"></script>
{% block footerJs %}{% endblock %}
</body>
</html>
